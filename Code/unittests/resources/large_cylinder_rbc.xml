<?xml version="1.0" ?>
<hemelbsettings version="3">
  <simulation>
    <step_length units="s" value="1.e-8" />
    <steps units="lattice" value="500" />
    <stresstype value="1" />
    <voxel_size units="m" value="1.4e-06" />
    <origin units="m" value="(-1.05e-05,-1.05e-05,-2.45e-05)" />
  </simulation>
  <geometry>
    <datafile path="large_cylinder.gmy" />
  </geometry>
  <inlets>
    <inlet>
      <condition type="pressure" subtype="cosine">
        <amplitude value="0.0" units="mmHg" />
        <mean value="0.1" units="mmHg" />
        <phase value="0" units="rad" />
        <period value="1" units="s" />
      </condition>
      <normal units="dimensionless" value="(0.0,0.0,1.0)" />
      <position units="m" value="(0.0,0.0,-2.4e-05)" />
      <flowextension>
        <length units="m" value="10e-6" />
        <radius units="m" value="10e-6" />
        <fadelength units="m" value="7e-06" />
      </flowextension>
      <insertcell template="default">
        <every units="s" value="2e-3"/>
        <offset units="s" value="40e-8"/>
      </insertcell>
    </inlet>
  </inlets>
  <outlets>
    <outlet>
      <condition type="pressure" subtype="cosine">
        <amplitude value="0." units="mmHg" />
        <mean value="0.0" units="mmHg" />
        <phase value="0" units="rad" />
        <period value="1" units="s" />
      </condition>
      <normal units="dimensionless" value="(0.0,0.0,-1.0)" />
      <position units="m" value="(0.0,0.0,2.4e-05)" />
      <flowextension>
        <length units="m" value="10e-6" />
        <radius units="m" value="10e-6" />
        <fadelength units="m" value="8e-06" />
      </flowextension>
    </outlet>
  </outlets>
  <visualisation>
    <centre units="m" value="(0.0,0.0,0.0)" />
    <orientation>
      <longitude units="deg" value="45.0" />
      <latitude units="deg" value="45.0" />
    </orientation>
    <display brightness="0.03" zoom="1.0" />
    <range>
      <maxvelocity units="m/s" value="0.1" />
      <maxstress units="Pa" value="0.1" />
    </range>
  </visualisation>
  <redbloodcells>
    <controller>
      <halo units="m" value="2e-6" />
      <boxsize units="m" value="100e-6" />
    </controller>
    <cells>
      <cell name="default">
        <moduli>
          <bending units="Nm" value="0e-10" />
          <surface units="LB" value="0e0" />
          <volume units="LB" value="1e0" />
          <dilation units="N/m" value="0e-1" />
          <strain units="N/m" value="0e-6" />
        </moduli>
        <interaction>
          <intensity units="N" value="0e-6" />
          <cutoffdistance units="LB" value="1.0" />
          <exponent value="2"/>
        </interaction>
        <shape mesh_path="red_blood_cell.txt" />
        <scale units="m" value="6e-06" />
      </cell>
    </cells>
  </redbloodcells>
  <initialconditions>
    <pressure>
      <uniform units="mmHg" value="0.0" />
    </pressure>
  </initialconditions>
  <monitoring>
    <incompressibility/>
  </monitoring>
</hemelbsettings>
