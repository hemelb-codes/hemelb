<?xml version="1.0" ?>
<hemelbsettings version="6">
  <simulation>
    <steps value="3000" units="lattice" />
    <step_length value="0.0001" units="s" />
    <voxel_size value="0.01" units="m" />
    <origin value="(0.0,0.0,0.0)" units="m" />
  </simulation>
  <geometry>
    <datafile path="./config.gmy" />
  </geometry>
  <initialconditions>
    <pressure>
      <uniform value="80.0" units="Pa"/>
    </pressure>
  </initialconditions>    
  <inlets>
    <inlet>
      <condition type="velocity" subtype="womersley">
        <pressure_gradient_amplitude value="1e3" units="Pa/m"/>
        <period value="0.0005" units="s"/>
        <womersley_number value="2" units="dimensionless"/>
        <radius value="0.1" units="m"/>
      </condition>
      <normal value="(0.0,0.0,1.0)" units="dimensionless" />
      <position value="(0,0,-0.05)" units="m" />
    </inlet>
  </inlets>
  <outlets>
    <outlet>
      <condition type="pressure" subtype="cosine">
        <amplitude value="0.0" units="Pa" />
        <mean value="0.0" units="Pa" />
        <phase value="0.0" units="rad" />
        <period value="0.0" units="s" />
      </condition>
      <normal value="(0.0,0.0,-1.0)" units="dimensionless" />
      <position value="(0.0,0.0,0.05)" units="m" />
    </outlet>
  </outlets>
</hemelbsettings>
